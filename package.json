{
  "name": "node-images-to-pdf",
  "version": "0.1.0",
  "description": "images-to-pdf",
  "keywords": [
    "images",
    "pdf",
    "images-to-pdf"
  ],
  "main": "lib/index.js",
  "scripts": {
    "start": "nodemon",
    "compile": "tsc --outDir lib",
    "lint": "eslint src/**/*.ts",
    "prettier": "prettier src/**/*.ts --write",
    "test": "jest ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "yarn lint",
      "yarn test",
      "yarn prettier",
      "yarn compile",
      "git add ."
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testTimeout": 10000,
    "preset": "ts-jest"
  },
  "author": {
    "name": "zenxds"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zenxds/images-to-pdf.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^4.3.0",
    "husky": "^1.3.1",
    "jest": "^27.4.0",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "express": "^4.17.1",
    "get-port": "5.1.1",
    "hbs": "^4.2.0",
    "pdf-merger-js": "^3.2.1",
    "puppeteer": "^12.0.0"
  }
}
